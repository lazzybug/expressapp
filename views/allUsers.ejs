<html>
    <head>
        <title></title>
        <link rel="stylesheet" href="/main.css">
    </head>
    <body>
        <div class="main">
            <div class="form">
                <h1>All Users</h1>
                <%users.forEach((user)=>{%>
                    <div class="user">
                        <h3>Name: <%=user.name%></h3>
                        <h4>Email: <%=user.email%></h4>
                        <h4>Email: <%=user.address%></h4>
                    </div>
                <%})%>
            </div>
        </div>
        <script src="main.js"></script>
    </body>
</html>